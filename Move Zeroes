class Solution {
    public void moveZeroes(int[] nums) {
       int idi = 0;
       for(int i = 0; i < nums.length; i++){
           if(nums[i] != 0){
               nums[idi] = nums[i];
               idi++;
           }
       } 
       while(idi< nums.length){
           nums[idi] = 0;
           idi++;
       }
    }
}
