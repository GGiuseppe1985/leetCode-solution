class Solution {
    public int[] sortedSquares(int[] nums) {
        int[] arr = new int[nums.length];
        int count = 0;
        for(int i = 0; i < nums.length; i++){
            arr[count] = nums[i] * nums[i];
            count++;
        }
        Arrays.sort(arr);
        return arr;
    }
}
